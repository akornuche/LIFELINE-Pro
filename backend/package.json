{
  "name": "lifeline-backend",
  "version": "1.0.0",
  "description": "LifeLine Pro - Backend API for Medical Coverage Platform",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "dev:sqlite": "cross-env DB_TYPE=sqlite npm run dev",
    "dev:postgres": "cross-env DB_TYPE=postgresql npm run dev",
    "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration",
    "db:init": "node src/database/init.js",
    "db:setup": "npm run db:init && npm run seed && npm run seed:admin",
    "db:setup:sqlite": "DB_TYPE=sqlite npm run db:setup",
    "db:setup:postgres": "DB_TYPE=postgresql npm run db:setup",
    "migrate": "node src/database/migrate.js",
    "migrate:rollback": "node src/database/migrate.js rollback",
    "seed": "node src/database/seed.js",
    "seed:admin": "node src/database/seedAdmin.js",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\""
  },
  "keywords": [
    "healthcare",
    "hmo",
    "insurance",
    "medical",
    "lifeline",
    "telemedicine"
  ],
  "author": "LifeLine Pro Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "socket.io": "^4.6.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "cross-env": "^10.1.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/database/migrations/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "verbose": true
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
